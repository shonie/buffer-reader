{
  "name": "bufferized",
  "version": "0.0.5",
  "description": "Zero-dependency filter that will read data from standard input and output a text + hex output to standard output.",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint ./",
    "test": "npm run test:unit && npm run test:benchmarks",
    "test:unit": "mocha --colors",
    "test:benchmarks": "node ./benchmarks/BufferReader.benchmark.js",
    "test:cli": "npm run lorem-ipsum | NODE_NO_WARNINGS=1 ./bin/cli.js",
    "lorem-ipsum": "lorem-ipsum --units words --count 10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shonie/bufferized.git"
  },
  "keywords": [
    "cli"
  ],
  "author": "shonie",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shonie/bufferized/issues"
  },
  "homepage": "https://github.com/shonie/bufferized#readme",
  "bin": {
    "bufferized": "./bin/cli.js"
  },
  "devDependencies": {
    "benchmark": "^2.1.4",
    "chai": "^4.2.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-mocha": "^5.2.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "lorem-ipsum": "^1.0.6",
    "mocha": "^5.2.0",
    "prettier": "^1.15.3"
  },
  "dependencies": {
    "@rauschma/stringio": "^1.4.0",
    "ramda": "^0.26.1",
    "window-size": "^1.1.1"
  },
  "engines": {
    "node": ">=11.6.0"
  }
}
